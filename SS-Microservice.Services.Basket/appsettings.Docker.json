{
  "app": {
    "name": "basket-service"
  },
  "GrpcSettings": {
    "ProductUrl": "http://product-service:5161"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "BasketDbContext": "Server=mysql; User ID=root; Password=Nms10102002; Database=basket_service"
  },
  "ConsulConfig": {
    "DiscoveryAddress": "http://consul:8500",
    "Name": "BasketService",
    "ID": "BasketService",
    "Port": 5216,
    "Address": "basket-service",
    "HealthCheckEndPoint": "api/healths/status"
  },
  "RabbitMqSettings": {
    "Uri": "rabbitmq",
    "UserName": "guest",
    "Password": "guest"
  },
  "Tokens": {
    "Key": "NguyenMinhSon10102002",
    "Issuer": "https://api.nms.com.vn"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithThreadId", "WithMachineName" ]
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "BasketService",
    "udpHost": "jaeger",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "elk": {
    "enabled": true,
    "url": "http://elasticsearch:9200",
    "indexFormat": "basket-service-{0:yyyy.MM.dd}",
    "basicAuthEnabled": false,
    "username": "user",
    "password": "secret"
  },
  "metrics": {
    "enabled": true,
    "prometheusEnabled": true,
    "prometheusFormatter": "protobuf",
    "env": "local",
    "interval": 5
  }
}