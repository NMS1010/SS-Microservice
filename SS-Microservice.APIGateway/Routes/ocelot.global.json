{
  "GlobalConfiguration": {
    "QoSOptions": {
      "ExceptionsAllowedBeforeBreaking": 2,
      "DurationOfBreak": 3000,
      "TimeoutValue": 5000
    },
    "LoadBalancerOptions": {
      "Type": "LeastConnection"
    },
    "HttpHandlerOptions": {
      "UseTracing": true
    },
    "BaseUrl": "https://localhost:7182",
    "ServiceDiscoveryProvider": {
      "Scheme": "http",
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul",
      "PollingInterval": 100
    }
  }
}